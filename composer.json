{
    "name" : "ctu-fee/udb-lib",
    "description" : "UDB Library - provides interface for accessing the user database",

    "require" : {
        "php" : ">=5.4.0",
        "zendframework/zend-ldap" : "~2.3",
        "zendframework/zend-stdlib" : "~2.3"
    },

    "require-dev" : {
        "zendframework/zend-config" : "~2.3",
        "phpunit/phpunit" : "~4.0"
    },

    "autoload" : {
        "psr-4" : {
            "Udb\\Domain\\" : "src/",
            "UdbTest\\Domain\\" : "tests/unit/",
            "UdbTest\\Domain\\Storage\\" : "tests/storage/tests/"
        }
    }
}